package com.example.demo.cmm.service;

import java.util.Optional;

import org.springframework.data.domain.Example;
import org.springframework.data.domain.Page;
import org.springframework.data.domain.Pageable;

public abstract class AbstractService<T> {
	public abstract long count();
	public abstract <T> boolean exists(Example<T> example);
	public abstract <T> Optional<T> findOne(Example<T> example);
	public abstract boolean existsById(long id);
	public abstract void deleteById(long id);
	public abstract <T> T save(T entity);
	public abstract Page<Long> findAll(Pageable pageable);
	public abstract <T> Iterable<T> saveAll(Iterable<T> entities);
}
