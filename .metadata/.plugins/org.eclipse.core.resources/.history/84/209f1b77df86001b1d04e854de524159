package com.example.demo.cmm.service;

import java.util.Optional;

import org.springframework.data.domain.Example;

public interface CommonService {
	public Iterable<Long> findAll();
	public long count();
	public void deleteById(long id);
	public boolean existsById(long id);
	public Optional<Long> findById(long id);
	public <Common> Optional<Common> findOne(Example<Common> example);
	public <Common> Common save(Common entity);
}
