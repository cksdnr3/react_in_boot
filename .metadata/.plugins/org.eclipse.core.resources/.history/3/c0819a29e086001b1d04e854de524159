package com.example.demo.cmm.service;

import java.util.Optional;

import org.springframework.data.domain.Example;
import org.springframework.data.domain.Page;
import org.springframework.data.domain.Pageable;

public abstract class AbstractService {
	public abstract long count();
	public abstract <User> boolean exists(Example<User> example);
	public abstract <User> Optional<User> findOne(Example<User> example);
	boolean abstract existsById(long id);
	public abstract void deleteById(long id);
	public abstract <User> User save(User entity);
	public abstract Page<Long> findAll(Pageable pageable);
	public abstract <User> Iterable<User> saveAll(Iterable<User> entities);
}
